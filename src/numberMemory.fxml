<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="NumberMemory">
   <children>
      <HBox alignment="CENTER" layoutY="356.0" prefHeight="44.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#goBackHome" text="Back to Home Screen" />
            <Button mnemonicParsing="false" onAction="#retryGame" text="Retry Game" />
         </children>
      </HBox>
      <Button fx:id="startButton" layoutX="278.0" layoutY="47.0" mnemonicParsing="false" onAction="#startGame" text="Start" />
      <Button fx:id="submitButton" layoutX="274.0" layoutY="187.0" mnemonicParsing="false" onAction="#compareNumbers" text="Sumbit" />
      <TextField fx:id="numberInput" layoutX="222.0" layoutY="155.0" onKeyTyped="#getNumberInput" />
      <Label fx:id="numberLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="98.0" layoutY="92.0" prefHeight="44.0" prefWidth="410.0">
         <font>
            <Font size="23.0" />
         </font>
      </Label>
      <Label fx:id="gameUpdates" alignment="CENTER" contentDisplay="CENTER" layoutX="82.0" layoutY="223.0" prefHeight="133.0" prefWidth="442.0" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="highScoreLabel" alignment="CENTER" layoutX="201.0" layoutY="14.0" prefHeight="27.0" prefWidth="204.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
