<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MatchPairs">
   <children>
      <HBox alignment="CENTER" layoutY="358.0" prefHeight="42.0" prefWidth="606.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#goBackHome" text="Back to Home Screen" />
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#retryGame" text="Retry Game" />
         </children>
      </HBox>
      <Button fx:id="startButton" layoutX="281.0" layoutY="337.0" mnemonicParsing="false" onAction="#startGame" text="Start" />
      <StackPane fx:id="stackpane" layoutX="13.0" layoutY="36.0" prefHeight="308.0" prefWidth="580.0" />
      <Label fx:id="gameOverLabel" contentDisplay="CENTER" layoutY="5.0" prefHeight="18.0" prefWidth="466.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="highScoreLabel" layoutX="466.0" layoutY="6.0" prefHeight="27.0" prefWidth="136.0" />
   </children>
</AnchorPane>
